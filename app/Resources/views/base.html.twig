<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>{% block title %}{{ app_name }}{% endblock %}</title>

  {% block stylesheet %}
  <!-- Bootstrap framework -->
  {#<link rel="stylesheet" href='{{asset("bootstrap/css/bootstrap.min.css")}}' />#}
  <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.no-icons.min.css" rel="stylesheet">
  <link rel="stylesheet" href='{{asset("bootstrap/css/bootstrap-responsive.min.css")}}' />
  <!-- Awesome font -->
  <link href="http://netdna.bootstrapcdn.com/font-awesome/3.2.0/css/font-awesome.css" rel="stylesheet">
  <!-- gebo blue theme-->
  <link rel="stylesheet" href='{{asset("css/blue.css")}}' />
  <!-- tooltips-->
  <link rel="stylesheet" href='{{asset("lib/qtip2/jquery.qtip.min.css")}}' />
  <!-- splashy icons -->
  <link rel="stylesheet" href='{{asset("img/splashy/splashy.css")}}' />
  <!-- main styles -->
  <link rel="stylesheet" href='{{asset("css/style.css")}}' />
  {% endblock %}

  {# <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=PT+Sans" /> #}
  <!-- Favicon -->
  <link rel="shortcut icon" href='{{asset("favicon.ico")}}' />

  <script>
  //* hide all elements & show preloader
  document.documentElement.className += 'js';
  </script>
</head>
<body>
  <div id="loading_layer" style="display:none"><img src='{{asset("img/ajax_loader.gif")}}' alt="" /></div>
  <div id="maincontainer" class="clearfix">
    <header>
      <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
          <div class="container-fluid">
            <a class="brand" href="#"><i class="icon-home icon-white"></i> {{ app_name }}</a>
            <ul class="nav user_menu pull-right">
              <li class="divider-vertical hidden-phone hidden-tablet"></li>
              {% if app.user %}
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><img src="{{asset('img/user_avatar.png')}}" alt="" class="user_avatar"> {{ app.user ? app.user.username : 'Anonymous'}} <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="{{path('fos_user_security_logout')}}"><i class="icon-off"></i> Logout</a></li>
                </ul>
              </li>
              {% else %}
              <li>
                <a href="{{ path('fos_user_security_login')}}"><img src="{{asset('img/user_avatar.png')}}" alt="" class="user_avatar"> Sign In</a>
              {% endif %}
              {% if app.debug %}
                <li class="divider-vertical hidden-phone hidden-tablet"></li>
                <li>
                  <a href="{{asset('developer.html')}}"><i class="icon-code icon-white"></i> Developer Guide</a>
                </li>
              {% endif %}
            </ul>

            <!-- header drop menu group disabled
            <ul class="nav" id="mobile-nav">
              <li class="dropdown">
                <a data-toggle="dropdown" class="dropdown-toggle" href="#"> menu <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="#">menu section</a></li>
                  <li><a href="#">menu section</a></li>
                  <li><a href="#">menu section</a></li>
                </ul>
              </li>
              <li class="dropdown">
                <a data-toggle="dropdown" class="dropdown-toggle" href="#"> menu <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="#">menu section</a></li>
                  <li><a href="#">menu section</a></li>
                  <li><a href="#">menu section</a></li>
                </ul>
              </li>
              <li class="dropdown">
                <a data-toggle="dropdown" class="dropdown-toggle" href="#"> menu <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="#">menu section</a></li>
                  <li><a href="#">menu section</a></li>
                  <li><a href="#">menu section</a></li>
                </ul>
              </li>
            </ul>
          </div> -->

        </div>
      </div>
    </header>

    <!-- main content -->
    <div id="contentwrapper">
      <div class="main_content">
        <div class="row-fluid">
          <div class="span12">
            <h3 class="heading">{% block heading %}Heading {% endblock %}</h3>
            {% block body %}
            {% endblock %}
          </div>
        </div>
      </div>
    </div>

    <!-- sidebar -->
    <a href="javascript:void(0)" class="sidebar_switch on_switch ttip_r" title="Hide Sidebar">Sidebar switch</a>
    <div class="sidebar">
      <div class="antiScroll">
        <div class="antiscroll-inner">
          <div class="antiscroll-content">
            <div class="sidebar_inner">
              <div id="side_accordion" class="accordion">

                <div class="accordion-group">
                  <div class="accordion-heading">
                    <a href="#collapseZero" data-parent="#side_accordion" data-toggle="collapse" class="accordion-toggle">
                      <i class="icon-flag"></i>
                      仪表盘
                    </a>
                  </div>
                  <div class="accordion-body collapse" id="collapseZero">
                    <div class="accordion-inner">
                      <ul class="nav nav-list">
                        <li class="active"><a href="#">简报</a></li>
                      </ul>
                    </div>
                  </div>
                </div>

                <div class="accordion-group">
                  <div class="accordion-heading">
                    <a href="#collapseOne" data-parent="#side_accordion" data-toggle="collapse" class="accordion-toggle" title="消费结账，客户挂账等">
                      <i class="icon-shopping-cart"></i>
                      收银
                    </a>
                  </div>
                  <div class="accordion-body collapse" id="collapseOne">
                    <div class="accordion-inner">
                      <ul class="nav nav-list">
                        <li><a href="#">订单管理</a></li>
                        <li><a href="#">历史订单</a></li>
                      </ul>
                    </div>
                  </div>
                </div>

                <div class="accordion-group">
                  <div class="accordion-heading">
                    <a href="#collapseTwo" data-parent="#side_accordion" data-toggle="collapse" class="accordion-toggle">
                      <i class="icon-heart"></i>
                      会员
                    </a>
                  </div>
                  <div class="accordion-body collapse" id="collapseTwo">
                    <div class="accordion-inner">
                      <ul class="nav nav-list">
                        <li><a href="#">查询</a></li>
                        <li><a href="#">积分</a></li>
                      </ul>
                    </div>
                  </div>
                </div>


                 <div class="accordion-group">
                  <div class="accordion-heading">
                    <a href="#collapseFour" data-parent="#side_accordion" data-toggle="collapse" class="accordion-toggle">
                      <i class="splashy-group_grey"></i>
                      供应商
                    </a>
                  </div>
                  <div class="accordion-body collapse" id="collapseFour">
                    <div class="accordion-inner">
                      <ul class="nav nav-list">
                        <li><a href="#">供应商列表</a></li>
                      </ul>
                    </div>
                  </div>
                </div>

                <div class="accordion-group">
                  <div class="accordion-heading">
                    <a href="#collapseFive" data-parent="#side_accordion" data-toggle="collapse" class="accordion-toggle">
                      <i class="icon-list"></i>
                      美容项目
                    </a>
                  </div>
                  <div class="accordion-body collapse" id="collapseFive">
                    <div class="accordion-inner">
                      <ul class="nav nav-list">
                        <li><a href="#">单项</a></li>
                        <li><a href="#">套餐(促销礼包)</a></li>
                        <li><a href="#">疗程</a></li>
                      </ul>
                    </div>
                  </div>
                </div>


              </div>
              <div class="push"></div>
            </div>
            <div class="sidebar_info"><span class="label label-info">{{ app_ver }}</span></div>
          </div>
        </div>
      </div>
    </div>

    {% block javascripts %}
    <script src='{{asset("js/jquery.min.js")}}'></script>
    <!-- smart resize event -->
    <script src='{{asset("js/jquery.debouncedresize.min.js")}}'></script>
    <!-- hidden elements width/height -->
    <script src='{{asset("js/jquery.actual.min.js")}}'></script>
    <!-- js cookie plugin -->
    <script src='{{asset("js/jquery_cookie.min.js")}}'></script>
    <!-- main bootstrap js -->
    <script src='{{asset("bootstrap/js/bootstrap.min.js")}}'></script>
    <!-- tooltips -->
    <script src='{{asset("lib/qtip2/jquery.qtip.min.js")}}'></script>
    <!-- fix for ios orientation change -->
    {# // <script src='{{asset("js/ios-orientationchange-fix.js")}}'></script> #}
    <!-- scrollbar -->
    <script src='{{asset("lib/antiscroll/antiscroll.js")}}'></script>
    <script src='{{asset("lib/antiscroll/jquery-mousewheel.js")}}'></script>
    <!-- mobile nav -->
    <script src='{{asset("js/selectNav.js")}}'></script>
    <!-- common functions -->
    <script src='{{asset("js/gebo_common.js")}}'></script>
    {% endblock %}

    <script>
    $(document).ready(function() {
      //* show all elements & remove preloader
      setTimeout('$("html").removeClass("js")',1000);
    });
    </script>
  </div>
</body>
</html>